    printf("            _____\n");
    printf("           /     \\\n");
    printf("          /  /\\   \\\n");
    printf("         |  /  \\   |\n");
    printf("         | |    |  |\n");
    printf("         | | ?? |  |\n");
    printf("         |  \\__/   |\n");
    printf("          \\       /\n");
    printf("           \\_____/\n");
    printf("             ||\n");
    printf("            /||\\\n");
    printf("           //||\\\\\n");
    printf("          // || \\\\\n");
    printf("             ||\n");
